package ${packageName}.service;

import ${packageName}.dao.${className}Dao;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import ${packageName}.service.base.AbstractService;

@Service
public class ${className}Service extends AbstractService {
	@Autowired
    private ${className}Dao ${name}Dao;

	public ${className}Service() {
		super();
	}

	@Override
	public ${className}Dao getDao() {
		return this.${name}Dao;
	}

}
